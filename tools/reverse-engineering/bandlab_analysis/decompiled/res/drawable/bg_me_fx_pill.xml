<?xml version="1.0" encoding="utf-8"?>
<ripple android:color="?android:colorControlHighlight"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/mask">
        <shape>
            <solid android:color="@color/technical_black_mask" />
            <corners android:radius="@dimen/grid_size_x2" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/technical_black_mask" />
            <corners android:radius="@dimen/grid_size_x2" />
            <padding android:left="@dimen/grid_size" android:top="@dimen/grid_size_quarter" android:right="@dimen/grid_size" android:bottom="@dimen/grid_size_quarter" />
        </shape>
    </item>
</ripple>