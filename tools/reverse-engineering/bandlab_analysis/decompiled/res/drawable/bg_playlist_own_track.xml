<?xml version="1.0" encoding="utf-8"?>
<ripple android:color="?android:colorControlHighlight"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/mask">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/grid_size" />
            <solid android:color="@color/technical_black_mask" />
        </shape>
    </item>
    <item android:top="@dimen/grid_size_eighth" android:bottom="@dimen/grid_size_eighth" android:start="@dimen/grid_size" android:end="@dimen/grid_size">
        <shape>
            <corners android:radius="@dimen/grid_size" />
            <solid android:color="@color/tint_green_20" />
        </shape>
    </item>
</ripple>