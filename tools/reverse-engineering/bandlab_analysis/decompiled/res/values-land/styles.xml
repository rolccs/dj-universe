<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Drumpad">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.0dip</item>
        <item name="numRows">2</item>
    </style>
    <style name="LooperEffectFilter">
        <item name="android:layout_marginEnd">@dimen/grid_size</item>
        <item name="layout_constraintBottom_toBottomOf">parent</item>
        <item name="layout_constraintEnd_toStartOf">@id/looper_effect_quant_bg</item>
        <item name="layout_constraintHorizontal_weight">@integer/looper_effect_weight</item>
        <item name="layout_constraintStart_toStartOf">parent</item>
        <item name="layout_constraintTop_toTopOf">parent</item>
    </style>
    <style name="LooperEffectGater">
        <item name="layout_constraintBottom_toBottomOf">parent</item>
        <item name="layout_constraintEnd_toEndOf">parent</item>
        <item name="layout_constraintHorizontal_weight">@integer/looper_effect_weight</item>
        <item name="layout_constraintStart_toEndOf">@id/looper_effect_quant_bg</item>
        <item name="layout_constraintTop_toTopOf">parent</item>
    </style>
    <style name="LooperEffectQuantization">
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">0.0dip</item>
        <item name="android:layout_marginEnd">@dimen/grid_size</item>
        <item name="layout_constraintBottom_toBottomOf">parent</item>
        <item name="layout_constraintEnd_toStartOf">@id/looper_effect_gater</item>
        <item name="layout_constraintHorizontal_weight">@integer/looper_effect_quant_weight</item>
        <item name="layout_constraintStart_toEndOf">@id/looper_effect_filter</item>
        <item name="layout_constraintTop_toTopOf">parent</item>
    </style>
    <style name="LooperEffectStutter16" parent="@style/LooperEffectStutter">
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">0.0dip</item>
        <item name="layout_constraintBottom_toBottomOf">parent</item>
        <item name="layout_constraintEnd_toStartOf">@id/looper_effect_stutter_32</item>
        <item name="layout_constraintHorizontal_chainStyle">packed</item>
        <item name="layout_constraintHorizontal_weight">1.0</item>
        <item name="layout_constraintStart_toStartOf">parent</item>
        <item name="layout_constraintTop_toBottomOf">@id/looper_effect_stutter_4</item>
        <item name="layout_constraintVertical_weight">1.0</item>
    </style>
    <style name="LooperEffectStutter32" parent="@style/LooperEffectStutter">
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">0.0dip</item>
        <item name="layout_constraintBottom_toBottomOf">parent</item>
        <item name="layout_constraintEnd_toEndOf">parent</item>
        <item name="layout_constraintHorizontal_chainStyle">packed</item>
        <item name="layout_constraintHorizontal_weight">1.0</item>
        <item name="layout_constraintStart_toEndOf">@id/looper_effect_stutter_16</item>
        <item name="layout_constraintTop_toBottomOf">@id/looper_effect_stutter_8</item>
        <item name="layout_constraintVertical_weight">1.0</item>
    </style>
    <style name="LooperEffectStutter4" parent="@style/LooperEffectStutter">
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">0.0dip</item>
        <item name="layout_constraintBottom_toTopOf">@id/looper_effect_stutter_16</item>
        <item name="layout_constraintEnd_toStartOf">@id/looper_effect_stutter_8</item>
        <item name="layout_constraintHorizontal_chainStyle">packed</item>
        <item name="layout_constraintHorizontal_weight">1.0</item>
        <item name="layout_constraintStart_toStartOf">parent</item>
        <item name="layout_constraintTop_toTopOf">parent</item>
        <item name="layout_constraintVertical_weight">1.0</item>
    </style>
    <style name="LooperEffectStutter8" parent="@style/LooperEffectStutter">
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">0.0dip</item>
        <item name="layout_constraintBottom_toTopOf">@id/looper_effect_stutter_32</item>
        <item name="layout_constraintEnd_toEndOf">parent</item>
        <item name="layout_constraintHorizontal_chainStyle">packed</item>
        <item name="layout_constraintHorizontal_weight">1.0</item>
        <item name="layout_constraintStart_toEndOf">@id/looper_effect_stutter_4</item>
        <item name="layout_constraintTop_toTopOf">parent</item>
        <item name="layout_constraintVertical_weight">1.0</item>
    </style>
    <style name="LooperEffectStutterLayout">
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">0.0dip</item>
        <item name="layout_constraintBottom_toBottomOf">@id/looper_effect_quant_bg</item>
        <item name="layout_constraintEnd_toStartOf">@id/divider</item>
        <item name="layout_constraintHorizontal_weight">2.0</item>
        <item name="layout_constraintStart_toStartOf">@id/looper_effect_quant_bg</item>
        <item name="layout_constraintTop_toTopOf">@id/looper_effect_quant_bg</item>
    </style>
    <style name="Widget.Design.TabLayout" parent="@style/Base.Widget.Design.TabLayout">
        <item name="tabGravity">center</item>
        <item name="tabMode">fixed</item>
    </style>
    <style name="Widget.MaterialComponents.MaterialCalendar.HeaderSelection" parent="@style/Widget.AppCompat.TextView">
        <item name="android:textAppearance">?textAppearanceHeadline6</item>
        <item name="android:textColor">?colorOnPrimary</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_gravity">start|center|top</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxLines">@integer/mtrl_calendar_selection_text_lines</item>
    </style>
    <style name="Widget.MaterialComponents.MaterialCalendar.HeaderToggleButton" parent="@style/Base.Widget.MaterialComponents.MaterialCalendar.HeaderToggleButton">
        <item name="android:layout_gravity">start|bottom|center</item>
    </style>
</resources>
