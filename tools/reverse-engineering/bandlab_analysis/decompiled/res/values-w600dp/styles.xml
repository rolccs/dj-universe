<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ASBottomButton" parent="@style/ASBaseBottomButton">
        <item name="android:minHeight">32.0dip</item>
    </style>
    <style name="ASButtonsFrame" parent="@style/ASBaseButtonsFrame" />
    <style name="ASPlaybackButton" parent="@style/ASImageButtonStyle">
        <item name="android:layout_width">40.0dip</item>
        <item name="android:layout_height">40.0dip</item>
    </style>
    <style name="ASPlaybackButton.Play" parent="@style/ASPlaybackButton">
        <item name="android:layout_marginStart">16.0dip</item>
        <item name="android:layout_marginEnd">16.0dip</item>
        <item name="layout_constraintEnd_toStartOf">@id/stForward</item>
        <item name="layout_constraintStart_toEndOf">@id/stBackward</item>
        <item name="layout_constraintTop_toTopOf">@id/stSpeed</item>
    </style>
    <style name="AdjustView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_marginStart">16.0dip</item>
        <item name="android:layout_marginEnd">16.0dip</item>
    </style>
