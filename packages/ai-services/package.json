    "redis": "^5.0.1",
    "pydantic": "^2.5.0",
    "scikit-learn": "^1.3.2"
  },
  "devDependencies": {
    "pytest": "^7.4.3",
    "pytest-asyncio": "^0.21.1",
    "black": "^23.11.0",
